<apex:page controller="C2">

  <apex:form >

     <apex:pageBlock title="Contact Info"> 
     <apex:pageBlockButtons >
     <apex:commandButton value="View Contacts" action="{!getcontacts}"/>
      </apex:pageBlockButtons> <br/>

     
     <apex:pageBlockTable value="{!conlist}" var="con">

     <apex:column headerValue="FirstName"  value="{!con.FirstName}"/>
     <apex:column headerValue="LastName"   value="{!con.LastName}"/> 
     <apex:column headerValue="Email"      value="{!con.Email}"/>
   



</apex:pageBlockTable>




</apex:pageBlock>




</apex:form>



</apex:page>